{
  "name": "Pop N' Lock",
  "author": "Gatongone",
  "dark": true,
  "editorScheme": "/PopNLock.xml",
  "colors": {
    "deepground": "#141421ff",
    "background": "#1c1c2dff",
    "hover": "#0c2d47",
    "selection": "#113a5c",
    "highlight": "#811da2ff",
    "normal": "#991f8aff",
    "text": "#bab2aeff",
    "border": "#313147ff"
  },
  "ui": {
    "*": {
      "foreground": "text",
      "background": "background",
      "lineSeparatorColor": "background",
      "separatorColor": "background",
      "selectionInactiveBackground": "selection",
      "selectionBackground": "selection",
      "selectedBackground": "selection",
      "hoveredBackground": "hover",
      "borderColor": "background"
    },
    "Button": {
      "default.startBackground": "highlight",
      "default.endBackground": "highlight",
      "startBackground": "highlight",
      "endBackground": "highlight"
    },
    "VersionControl": {
      "Log.Commit": {
        "currentBranchBackground": "background",
        "unmatchedForeground": "deepground"
      }
    },
    "Borders.color": "background",
    "Borders.ContrastBorderColor": "border",
    "ToolWindow.Header.inactiveBackground": "background",
    "CompletionPopup.Advertiser.foreground": "highlight",
    "Link.activeForeground": "highlight",
    "TextArea.inactiveBackground": "deepground",
    "Component.borderColor": "border",
    "Table.gridColor": "highlight",
    "Table.stripeColor": "deepground",
    "TableHeader.bottomSeparatorColor": "border",
  }
}